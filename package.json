{
  "name": "flofun-api",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/cpenarrieta/flofun-api.git",
  "author": "Cristian Penarrieta <cristian.penarrieta@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev:build": "webpack -w",
    "dev": "NODE_ENV=development nodemon dist/index.bundle.js",
    "clean": "rimraf dist && rimraf node_modules",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "NODE_ENV=test mocha $(find __tests__ -name *.test.js) --colors --compilers js:babel-register",
    "prettier": "prettier --no-semi --single-quote --print-width 100 --trailing-comma all --write '{src,__{tests,mocks}__}/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --single-quote --print-width 100 --trailing-comma all --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "0.16.2",
    "body-parser": "1.17.2",
    "compression": "1.7.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "4.15.4",
    "express-validation": "^1.0.2",
    "helmet": "3.8.1",
    "jsonwebtoken": "7.4.2",
    "mongoose": "4.11.6",
    "morgan": "1.8.2",
    "passport": "0.3.2",
    "passport-jwt": "2.2.1",
    "pretty-error": "^2.1.1",
    "raven": "^2.1.1",
    "twilio": "3.6.1"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.1",
    "eslint": "4.4.1",
    "eslint-config-equimper": "2.2.1",
    "eslint-config-prettier": "^2.3.0",
    "faker": "^4.1.0",
    "mocha": "^3.5.0",
    "nodemon": "1.11.0",
    "prettier": "^1.5.3",
    "rimraf": "2.6.1",
    "supertest": "^3.0.0",
    "webpack": "2.4.1",
    "webpack-node-externals": "1.6.0"
  }
}
